# LAMP
alias h='cd ~'
alias c='clear'
alias bashrc='sudo vim ~/.bash_aliases'
alias rmr='rm -rf'
alias ver='lsb_release -a'
alias rn='sudo /etc/init.d/networking restart'
alias ra='sudo service apache2 restart'
alias my='mysql -u drupal'
alias b='cd ..'
alias hi='history | grep'
alias www='cd /var/www'
alias mysql='sudo mysql -u root -p'

# Git
alias gits='git status'
alias gitss='git status .'
alias gitc='git commit -m'
alias gitp='git push'
alias gita='git add .'

# Drush
alias cr='/var/www/drupal-admin-ui-outline/demo/vendor/drush/drush/drush cr'

# AD
alias ad='cd /var/www/drupal-admin-ui-outline'
alias demo='cd /var/www/drupal-admin-ui-outline/demo'
alias dr='cd /var/www/drupal-admin-ui-outline/demo/docroot'
alias pack='cd /var/www/drupal-admin-ui-outline/packages'
alias drush='/var/www/drupal-admin-ui-outline/demo/vendor/drush/drush/drush'

# OL
alias vo='cd /var/www/vue-outline'
alias vostart='cd ~/vue-outline && npm run start && npm run serve'
alias ol='cd ~/vue-outline/packages/outline'
alias olapi='cd ~/vue-outline/packages/api'
alias old='cd  /var/www/vue-outline/packages/drupal'
alias olscr='cd ~/vue-outline/scripts'
alias o='cd ~/vue-outline/packages/drupal/outline'
alias olg='cd ~/vue-outline/packages/drupal/web/modules/contrib/outline/modules/outline_graphql'
alias olcore='cd ~/vue-outline/packages/drupal/web/core'
alias olsites='cd ~/vue-outline/packages/drupal/web/sites'
alias olm='cd ~/vue-outline/packages/drupal/web/modules'
alias olmc='cd ~/vue-outline/packages/drupal/web/modules/contrib'
alias olmcu='cd ~/vue-outline/packages/drupal/web/modules/custom'
alias oldef='cd ~/vue-outline/packages/drupal/web/sites/default'
alias olset='vi ~/vue-outline/packages/drupal/web/sites/default/settings.php'
alias olclone='git clone --branch 8.x-1.x git@git.drupal.org:project/outline.git'
alias voclone='git clone git@github.com:captaindav/vue-outline'


